    "@vitest/coverage-v8": "0.34.3",
    "audit-ci": "6.6.1",
    "c8": "8.0.1",
    "eslint": "8.48.0",
    "eslint-config-react-app": "7.0.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "react": "18.2.0",
