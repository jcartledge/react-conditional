{
  "name": "@jcartledge/react-conditional",
  "repository": "github:jcartledge/react-conditional",
  "version": "1.0.9",
  "description": "Simple conditional component for React.",
  "homepage": "https://jcartledge.github.io/react-conditional/",
  "main": "dist/Conditional.js",
  "types": "dist/Conditional.d.ts",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clean && tsc",
    "check-deps": "pnpm audit",
    "clean": "rimraf dist",
    "coverage": "CI=true vitest --coverage",
    "docs": "typedoc --out docs src/Conditional.tsx",
    "lint": "eslint src",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "vitest"
  },
  "author": "jcartledge@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@types/react": "18.2.46",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "0.34.6",
    "audit-ci": "6.6.1",
    "c8": "8.0.1",
    "eslint": "8.51.0",
    "eslint-config-react-app": "7.0.1",
    "husky": "8.0.3",
    "jsdom": "22.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "typedoc": "0.25.6",
    "typescript": "5.2.2",
    "vite": "5.0.11",
    "vitest": "0.34.6"
  },
  "pnpm": {
    "overrides": {
      "postcss@<8.4.31": ">=8.4.31",
      "semver@<5.7.2": ">=5.7.2",
      "vite@>=4.4.0 <4.4.12": ">=4.4.12",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "@adobe/css-tools@<4.3.2": ">=4.3.2"
    }
  }
